/* Губанова Александра, вариант 7
Задача 1.3. Удалить справа подряд идущие четные цифры в записи числа (2567802 => 2567).*/

#include <stdio.h>
#include <windows.h>

int main()
{
  SetConsoleCP(1251);
  SetConsoleOutputCP(1251);
  
  unsigned long a;

  printf("Введите число\n");
  scanf("%lu", &a);
  while (a>0 && a%2==0)
  {
    a/=10;
  }
  printf("Конечный результат:%lu\n",a);
   
  system("PAUSE");
  return 0;
}


